<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/templateTabhostLayout" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:gravity="center_horizontal"
	android:background="@drawable/bg_green" android:orientation="vertical">
	<LinearLayout android:id="@+id/tabhostLayout"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center_horizontal" android:orientation="horizontal">		
<!--		<ImageView android:id="@+id/tabListIView" android:src="@drawable/list_button_rolver"-->
<!--			android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>-->
<!--		<ImageView android:id="@+id/tabMapIView" android:src="@drawable/map_button"-->
<!--			android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>-->
	</LinearLayout>
	<LinearLayout android:id="@+id/tabhostButtonLayout"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:gravity="center_vertical" android:background="@drawable/bg_navigation_2_bar"
		android:orientation="horizontal">
		<ImageView android:id="@+id/tabBack"
			android:paddingLeft="5dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></ImageView>
		<TextView android:id="@+id/tabText" android:gravity="center"
			android:textColor="@color/BLACK" android:textStyle="bold" android:layout_marginLeft="-55dip"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="" android:src="@drawable/button_cust_tab_next"></TextView>
		<ImageView android:id="@+id/tabNext"
			android:layout_marginLeft="-55dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content"></ImageView>
	</LinearLayout>
</LinearLayout>
